{
  "routes": [
    {
      "src": "/sitemap\\.xml",
      "dest": "/server/index.cjs",
      "methods": ["GET"]
    },
    {
      "src": "/robots\\.txt", 
      "dest": "/server/index.cjs",
      "methods": ["GET"]
    },
    {
      "src": "/api/(.*)",
      "dest": "/server/index.cjs"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_SITE_URL": "https://study-vault2.vercel.app"
  }
} 